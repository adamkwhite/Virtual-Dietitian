# Environment variables for local testing
# Copy this file to .env and fill in your values

# Feature flag: Enable USDA API fallback
ENABLE_USDA_API=true

# USDA FoodData Central API key
# Get your key at: https://fdc.nal.usda.gov/api-key-signup.html
USDA_API_KEY=YOUR_API_KEY_HERE

# Environment
ENVIRONMENT=local

# Nutrition database path (relative to this directory)
NUTRITION_DB_PATH=./nutrition_db.json
